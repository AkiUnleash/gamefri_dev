{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "allowJs": true,
        "strictNullChecks": true,
        "module": "ES6",
        "target": "ES2015",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
    },
    "include": [
        "src",
    ],
    "exclude": [
        "node_modules"
    ],
}
